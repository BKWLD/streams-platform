<!-- Table Head -->
<thead>
<tr>

    {% if table.options.sortable %}
        <th>
            <i class="fa fa-reorder text-muted"></i>
        </th>
    {% endif %}

    {% if not table.actions.empty() %}
        <th>
            <input data-toggle="all" type="checkbox">
        </th>
    {% endif %}

    {% for column in table.columns %}
        <th>
            {{ table_heading(table, column) }}
        </th>
    {% endfor %}

    <th>&nbsp;</th>
</tr>
</thead>
