<!-- Table Head -->
<thead>
<tr>

    {% if options.sortable %}
        <th>
            <i class="fa fa-reorder text-muted"></i>
        </th>
    {% endif %}

    {% if not actions.empty() %}
        <th>
            <input data-toggle="all" type="checkbox">
        </th>
    {% endif %}

    {% for column in columns %}
        <th>
            {{ table_column_header(table, column) }}
        </th>
    {% endfor %}

    <th>&nbsp;</th>
</tr>
</thead>
