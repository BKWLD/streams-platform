{
    "name": "anomaly/streams-platform",
    "description": "Streams platform core library.",
    "authors": [
        {
            "name": "AnomalyLabs",
            "email": "projects@anomaly.is"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:anomalylabs/framework"
        }
    ],
    "require": {
        "laravel/framework": "streams-compatible-dev",
        "anomaly/lexicon": "dev-master",
        "artdarek/oauth-4-laravel": "dev-master",
        "aws/aws-sdk-php": "dev-master",
        "barryvdh/laravel-debugbar": "1.*",
        "barryvdh/laravel-ide-helper": "1.11.*@dev",
        "coffeescript/coffeescript": "dev-master",
        "dropbox/dropbox-sdk": "1.1.*",
        "cebe/markdown": "*",
        "doctrine/dbal": "dev-master",
        "guzzlehttp/guzzle": "~4.0",
        "intervention/image": "dev-master",
        "illuminate/html": "~5.0",
        "jenssegers/agent": "dev-master",
        "kriswallsmith/assetic": "1.2.*@dev",
        "leafo/scssphp": "dev-develop",
        "league/event": "dev-master",
        "lmammino/jsmin4assetic": "1.0.*@dev",
        "natxet/CssMin": "dev-master",
        "nesbot/Carbon": "1.*",
        "netcarver/textile": "3.6.*@dev",
        "oyejorge/less.php": "~1.5",
        "ptachoire/cssembed": "dev-master",
        "phpseclib/phpseclib": "dev-master",
        "rackspace/php-opencloud": "dev-master",
        "robclancy/presenter": "1.3.*",
        "filp/whoops": "~1.0",
        "symfony/yaml": "2.7.*@dev",
        "laracasts/commander": "~1.0",
        "way/generators": "~3.0",
        "dimsav/laravel-translatable": "dev-master"
    },
    "require-dev": {
        "anomaly/streams": "dev-master",
        "benconstable/phpspec-laravel": "dev-fix-laravel-5-compatibility"
    },
    "autoload": {
        "psr-0": {
            "Anomaly\\Streams\\Platform": "src/"
        },
        "files": [
            "resources/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
