{
    "name": "streams/platform",
    "description": "Streams Platform",
    "authors": [
        {
            "name": "AnomalyLabs",
            "email": "projects@anomaly.is"
        }
    ],
    "require": {
        "cebe/markdown": "*",
        "nesbot/Carbon": "1.*",
        "illuminate/html": "~5.0",
        "cartalyst/sentry": "2.1.*",
        "oyejorge/less.php": "~1.5",
        "laravel/framework": "~5.0",
        "natxet/CssMin": "dev-master",
        "leafo/scssphp": "dev-develop",
        "robclancy/presenter": "1.3.*",
        "anomaly/lexicon": "dev-master",
        "netcarver/textile": "3.6.*@dev",
        "ptachoire/cssembed": "dev-master",
        "intervention/image": "dev-master",
        "barryvdh/laravel-debugbar": "1.*",
        "kriswallsmith/assetic": "1.2.*@dev",
        "lmammino/jsmin4assetic": "1.0.*@dev",
        "artdarek/oauth-4-laravel": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "henrikbjorn/phpspec-code-coverage": "~0.2",
        "benconstable/phpspec-laravel": "~1.0"
    },
    "autoload": {
        "psr-0": {
            "Streams\\Platform": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
