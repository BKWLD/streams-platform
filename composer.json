{
    "name": "anomaly/streams-platform",
    "description": "Streams platform core library.",
    "authors": [
        {
            "name": "AnomalyLabs",
            "email": "projects@anomaly.is"
        }
    ],
    "require": {
        "laravel/framework": "~5.0",
        "illuminate/html": "~5.0",
        "twig/twig": "1.16.*@dev",
        "barryvdh/laravel-debugbar": "1.*",
        "barryvdh/laravel-ide-helper": "1.11.*@dev",
        "coffeescript/coffeescript": "1.3.1",
        "michelf/php-markdown": "1.4.1",
        "aptoma/twig-markdown": "0.2.*",
        "guzzlehttp/guzzle": "~4.0",
        "kriswallsmith/assetic": "1.2.*@dev",
        "leafo/scssphp": "0.1.0 ",
        "lmammino/jsmin4assetic": "1.0.*@dev",
        "natxet/CssMin": "3.0.*@dev",
        "nesbot/Carbon": "1.*",
        "oyejorge/less.php": "~1.5",
        "ptachoire/cssembed": "1.0.2",
        "phpseclib/phpseclib": "0.3.*@dev",
        "robclancy/presenter": "1.3.*",
        "filp/whoops": "~1.0",
        "symfony/yaml": "2.7.*@dev",
        "dimsav/laravel-translatable": "5.0.*@dev",
        "venturecraft/revisionable": "1.*"
    },
    "require-dev": {
        "anomaly/streams": "dev-master",
        "phpspec/phpspec": "~2.0",
        "benconstable/phpspec-laravel": "~1.0",
        "henrikbjorn/phpspec-code-coverage": "~0.2"
    },
    "autoload": {
        "psr-4": {
            "Anomaly\\Streams\\Platform\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
